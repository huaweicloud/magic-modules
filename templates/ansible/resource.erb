        return fetch
<%  unless externals.empty? -%>
<%   if object.msg_prefix('get') -%>
    fetch = fetch.get(<%= quote_string(object.msg_prefix('get')) %>)
<%   end -%>

<%   externals.each do |p| -%>
    r = <%= p.property_class[-1] -%>ExProperty(session).fetch()
    if isinstance(r, dict):
        fetch.update(r)

<%   end -%>
    return fetch
<%  else -%>
    return fetch.get(<%= quote_string(object.msg_prefix('get')) %>)
<%  end -%>
<% end -%>



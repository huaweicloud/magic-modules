def get_resource(session):
    link = self_link(session)
    # the link will include Nones if required format parameters are missed
    if re.search('/None/|/None$', link):
        return None

<% unless object.msg_prefix('get') || (not externals.empty?) -%>
    return <%= method_fetch %>
<% else -%>
    fetch = <%= method_fetch %>
